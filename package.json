{
  "name": "date-holidays",
  "version": "0.1.1",
  "description": "worldwide holidays",
  "main": "lib/Holidays.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "date-easter": "^0.2.2",
    "mergee": "^0.2.3",
    "moment-timezone": "^0.5.0",
    "moonbeams": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "js-yaml": "^3.4.6",
    "jsdox": "latest",
    "mocha": "^2.3.4"
  },
  "scripts": {
    "test": "mocha test/*.mocha.js",
    "prepublish": "npm run yaml && npm run lint && npm test",
    "yaml": "node make/yaml2json.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "eslint --ignore-path .gitignore */*.js **/*.js",
    "watch": "watch-run -p data/*.yaml npm run yaml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/date-holidays.git"
  },
  "keywords": [
    "holidays"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/commenthol/date-holidays/issues"
  },
  "homepage": "https://github.com/commenthol/date-holidays",
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  }
}
